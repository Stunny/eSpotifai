package view;

import java.awt.Color;
/**
 * Clase de definició dels colors escollits com a constants.
 * @author Elna Cabot, Miguel Diaz, Marc Millan, Alejandro Vogel, Marta Zapatero
 * @version 1.0
 *
 */
public class CustomColor {
	
	final static Color background = Color.decode("#282324"); // GRIS FOSC
	final static Color secondary = Color.decode("#353332");;
	final static Color selected = Color.decode("#4C4B49");; 
	final static Color highlight = Color.decode("#6171CE");;
	final static Color icon = Color.decode("#8E8B8A");;
	
	/**
	 * Constructor de la paleta de colors.
	 */
	public CustomColor(){
	}
	
}
